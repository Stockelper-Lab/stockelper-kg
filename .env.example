# OpenDART API Key
# Get your key from: https://opendart.fss.or.kr/
OPEN_DART_API_KEY=

# Korea Investment & Securities API
# Register at: https://apiportal.koreainvestment.com/
KIS_APP_KEY=
KIS_APP_SECRET=
KIS_ACCESS_TOKEN=
KIS_ACCESS_NUMBER=

# Account Information (Optional)
KIS_ACCOUNT_NUMBER=
KIS_ACCOUNT_CODE=

# Virtual Trading Mode
KIS_VIRTUAL=true

# Neo4j Database
# Local development
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=password

# Docker environment (use container name)
# NEO4J_URI=bolt://stockelper-neo4j:7687

# MongoDB (for competitor data)
# Local development
DB_URI=mongodb://localhost:27017
DB_NAME=stockelper
DB_COLLECTION_NAME=competitors

# Docker environment (use container name)
# DB_URI=mongodb://stockelper-mongodb:27017
